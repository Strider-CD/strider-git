{
  "name": "strider-git",
  "version": "1.0.3",
  "description": "Strider provider for git repositories",
  "main": "worker.js",
  "scripts": {
    "lint": "eslint *.js lib",
    "test": "npm run lint && mocha -R spec",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-git"
  },
  "keywords": [
    "strider",
    "plugin",
    "provider",
    "git"
  ],
  "engines": {
    "node": ">=10.13.0"
  },
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "strider": {
    "id": "git",
    "title": "Git",
    "type": "provider",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "inline_icon": "code-fork",
    "config": true
  },
  "dependencies": {
    "debug": "^4.0.0",
    "fs-extra": "^9.0.0",
    "gitane": "~0.3.1",
    "shell-escape": "0.0.1",
    "strider-docker-gitane-camo": "~0.0.1"
  },
  "devDependencies": {
    "standard-version": "^8.0.0",
    "eslint": "^7.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^8.0.0"
  }
}
